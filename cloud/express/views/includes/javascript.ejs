setTimeout(function() {
  <% if(user != null && user.objectId != null) { %>
    mixpanel.alias("<%= user.objectId %>", mixpanel.get_distinct_id()) 
    mixpanel.people.set({
      ID: "<%= user.objectId %>",
      $name: "<%= user.name %>",
      $email: "<%= user.email %>",
    })
  <% } %>
  mixpanel.track('WEB: Page Viewed', {
      'Name' : config.title,
      'URL' : window.location.pathname
  })
}, 500)
